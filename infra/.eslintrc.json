{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": ["standard-with-typescript", "prettier"],
  "plugins": ["prettier", "@pulumi"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "warn",
    "no-new": "off"
  }
}
